
--getProfile
SELECT *  
FROM USER_PROFILE UP
INNER JOIN "USER_ TABLA SEGUN QUE TE PIDAN" OU ON UP.ID_USER = OU.ID_USER
WHERE ID_USER = "USER"
--addTags (en bucle)
insert into "INTEREST_ TABLA SEGUN QUE TE PIDAN" VALUES("ID_USER","ID_TAG")
--getAllInterestTable
select ID_INTEREST,INTEREST.TAGNAME from "INTEREST_ TABLA SEGUN QUE TE PIDAN" INTE
INNER JOIN INTEREST ON INTE.ID_INTEREST = INTEREST.ID_INTEREST
--editAge
update table USER_PROFILE set AGE = "EDAD QUE TE PASEN" WHERE ID_USER = "USER QUE TE PASEN"
--editDescription
update table "USER_ TABLA SEGUN TE PIDAN" set DESCRIPTION = "DESCRIPCION QUE TE PASEN" WHERE ID_USER = "USER QUE TE PASEN"
--editStudiyng
update table "USER_ TABLA SEGUN TE PIDAN" set DESCRIPTION = "studying QUE TE PASEN" WHERE ID_USER = "USER QUE TE PASEN"
--editSearch
update table "USER_ TABLA SEGUN TE PIDAN" set SEARCHING = "SEARCH QUE TE PASEN" WHERE ID_USER = "USER QUE TE PASEN"
--editWork
update table "USER_ TABLA SEGUN TE PIDAN" set WORKING = "WORK QUE TE PASEN" WHERE ID_USER = "USER QUE TE PASEN"
--editGender
update table "USER_ TABLA SEGUN TE PIDAN" set GENDER = "GENDER QUE TE PASEN" WHERE ID_USER = "USER QUE TE PASEN"
